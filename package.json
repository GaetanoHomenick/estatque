{
  "name": "estatque",
  "version": "1.0.0",
  "description": "Advanced typescript localization support for VueJS",
  "keywords": [
    "incidunt",
    "locale",
    "translation",
    "suscipit",
    "expedita",
    "minima",
    "ea"
  ],
  "main": "./lib/locale.directive.js",
  "types": "./lib/locale.directive.d.ts",
  "author": "JakeHintz",
  "contributor": [
    {
      "name": "Sebastian Werner",
      "email": "s.werner@sebastian-software.de"
    }
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/GaetanoHomenick/estatque.git"
  },
  "bugs": {
    "url": "https://github.com/GaetanoHomenick/estatque.git/issues"
  },
  "dependencies": {
    "intl": "^1.2.5",
    "intl-format-cache": "^2.0.5",
    "intl-messageformat": "^1.3.0",
    "intl-relativeformat": "^1.3.0",
    "lodash": "^4.17.4",
    "vue": "^2.2.6"
  },
  "scripts": {
    "build": "gulp build",
    "pretest": "npm run build",
    "test": "ava",
    "test:watch": "ava --watch",
    "release": "release-it --github.release --npm.publish --non-interactive",
    "release-minor": "release-it --github.release --npm.publish --non-interactive --increment minor",
    "release-major": "release-it --github.release --npm.publish --non-interactive --increment major",
    "depupdate": "updtr"
  },
  "devDependencies": {
    "@types/node": "^7.0.12",
    "ava": "^0.19.1",
    "babel-register": "^6.24.1",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.1.6",
    "merge2": "^1.0.3",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2",
    "updtr": "^1.0.0"
  }
}